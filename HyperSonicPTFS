task.spawn(function()
while _G.speedom do
        task.wait()
    if (game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")) then
    km.Text = string.format("%.1f",((game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity.Magnitude*1.833)*1.839))
    maxu.Text = string.format("%.2f",(game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity.Magnitude*1.833)/(1.958)/340.27777778)
    kts.Text = string.format("%.1f",game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity.Magnitude*1.833)
    end
end
end)

Players = game:GetService("Players")
IYMouse = Players.LocalPlayer:GetMouse()
local UIS = game:GetService("UserInputService")

function getRoot(char)
	local rootPart = char:FindFirstChild('HumanoidRootPart') or char:FindFirstChild('Torso') or char:FindFirstChild('UpperTorso')
	return rootPart
end

function vFLY(vfspeed)
	repeat wait() until Players.LocalPlayer and Players.LocalPlayer.Character and getRoot(Players.LocalPlayer.Character) and Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
	repeat wait() until IYMouse

	local T = getRoot(Players.LocalPlayer.Character)
	CONTROL = {F = 0}
	lCONTROL = {F = 0}
	local SPEED = 0

	local function FLY()
		FLYING = true
		local BG = Instance.new('BodyGyro')
		local BV = Instance.new('BodyVelocity')
		BG.P = 9e4
		BG.Parent = T
		BV.Parent = T
		BG.maxTorque = Vector3.new(9e9, 9e9, 9e9)
		BG.cframe = T.CFrame
		BV.velocity = Vector3.new(0, 0, 0)
		BV.maxForce = Vector3.new(9e9, 9e9, 9e9)
		task.spawn(function()
			repeat wait()
				if CONTROL.F ~= 0 then
				    SPEED = 50
				elseif not CONTROL.F and SPEED ~= 0 then
				    SPEED = 0
				end
		     	if CONTROL.F ~= 0 or CONTROL.F == 0 then
					BV.velocity = ((workspace.CurrentCamera.CoordinateFrame.lookVector * CONTROL.F) + ((workspace.CurrentCamera.CoordinateFrame * CFrame.new(0, 0, 0).p) - workspace.CurrentCamera.CoordinateFrame.p)) * SPEED
				else
					BV.velocity = Vector3.new(0, 0, 0)
			    end
				BG.cframe = workspace.CurrentCamera.CoordinateFrame
			until not FLYING
			BG:Destroy()
			BV:Destroy()
			if Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid') then
				Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid').PlatformStand = false
			end
		end)
	end
			CONTROL.F = vfspeed
	FLY()
end

function NOFLY()
	FLYING = false
	if Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid') then
		Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid').PlatformStand = false
	end
end

local onekts = 93.511
_G.OnOff = false
_G.ended = false
_G.toend = false

task.spawn(function()
    while true do
        task.wait()
        if (game.Players.LocalPlayer:FindFirstChild("PlayerGui")) then
        repeat wait() until game.Players.LocalPlayer.PlayerGui:FindFirstChild("PlaneGui")
        if game.Players.LocalPlayer.PlayerGui.PlaneGui.Panel.Speed.Value.Text == "Cruising" or game.Players.LocalPlayer.PlayerGui.PlaneGui.Panel.Speed.Value.Text == "0" then
        else
        local s = game.Players.LocalPlayer.PlayerGui.PlaneGui.Panel.Speed.Value.Text
        local speed = s:gsub("kts","")
        if tonumber(speed) >= 1200 then
        available.Visible = true
        else available.Visible = false
        end
        if game.Players.LocalPlayer.PlayerGui.PlaneGui.Enabled == true then
        main2.Visible = true
        else main2.Visible = false hypercheck.Visible = false
        end
        end
        end
    end
end)

UIS.InputBegan:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.N then
if (game.Players.LocalPlayer:FindFirstChild("PlayerGui")) then
repeat wait() until game.Players.LocalPlayer.PlayerGui:FindFirstChild("PlaneGui")
if game.Players.LocalPlayer.PlayerGui.PlaneGui.Panel.Speed.Value.Text == "Cruising" or game.Players.LocalPlayer.PlayerGui.PlaneGui.Panel.Speed.Value.Text == "0" then
else
if (game.Players.LocalPlayer.PlayerGui.PlaneGui.Enabled == true) then
local s = game.Players.LocalPlayer.PlayerGui.PlaneGui.Panel.Speed.Value.Text
local perc = game.Players.LocalPlayer.PlayerGui.PlaneGui.Panel.Throttle.Bar.Percent.Text
local speed = s:gsub("kts","")
local a = speed
if _G.OnOff == true and (game.Players.LocalPlayer.Character:FindFirstChild("Humanoid")) then
_G.OnOff = false
NOFLY()
hypercheck.Visible = false

elseif _G.OnOff == false and tonumber(speed) >= 1200 and (game.Players.LocalPlayer.Character:FindFirstChild("Humanoid")) then
    if _G.toend == true then
        _G.ended = true
    elseif _G.toend == false then
        hypercheck.Visible = true
    for i=1,1500 do
    if (game.Players.LocalPlayer.Character:FindFirstChild("Humanoid")) and (game.Players.LocalPlayer.PlayerGui.PlaneGui.Enabled == true) then
    vFLY(a/onekts)
    hypercheck.Text = string.format("%.0f",a).."/4000"
    if tonumber(a) >= 4000 then
        break
        else
        task.wait(0.025)
    if perc == "100%" then
    a = a+8
    elseif perc >= "80%" then
    a = a+7
    elseif perc >= "60%" then
    a = a+6
    elseif perc >= "40%" then
    a = a+5
    elseif perc >= "20%" then
    a = a+4
    elseif perc < "20%" and perc ~= "0%" then
    a = a+3
    end
    end
    _G.toend = true
    if _G.ended then hypercheck.Visible = false NOFLY() _G.ended = false _G.toend = false _G.OnOff = false break end
    else hypercheck.Visible = false NOFLY() _G.OnOff = false _G.ended = false _G.toend = false break end
end
_G.OnOff = true
_G.ended = false
_G.toend = false
end
end
else
game.Players.LocalPlayer.PlayerGui.PlaneGui.Panel.Speed.Value.Text = "0kts"
_G.OnOff = false _G.ended = false _G.toend = false
hypercheck.Visible = false
NOFLY()
end
end
end
end
end)
